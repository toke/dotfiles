#!/usr/bin/mksh

MKHOME="${HOME}/.mksh"
source "${MKHOME}/helper.mksh"
source "${MKHOME}/env"

## Function lookup path
[[ -e "${MKHOME}/func.d" ]] && export FPATH="${MKHOME}/func.d"

## Create data dir if not existent
[[ ! -e  $HOME/.local/data/mksh ]] && mkdir -p "$HOME/.local/data/mksh"

case "$-" in
  *i*)
    source "${MKHOME}/interactive"
    ;;
esac
